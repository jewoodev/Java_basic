package mycom.test;

public class Main2 {
	public static void main(String[] args) {
		int i = 10;
		double d = 17.5;
		
		double result = 0.0;
		result = i;//double = int; (암묵적 형변환을 지원)
		//cast 연산자 = 형변환 연산자
		result = (int)d; //int(4B) = result(8B)
		
		int ch =  'c'; //문자 --> 숫자
		System.out.println(ch);
		System.out.println("result="+i);
	}
};
